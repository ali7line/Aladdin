/*  =======================================================  */
/*  VARIBALE WITH UNITS II                                   */
/*  This input file tests the variables with different Units */
/*  =======================================================  */
 
/* ====================*/
/* UNITS of TIME       */
/* ====================*/

/* case 1: SI system   */

print " TIME UNITS \n";
x =  1 sec;
y =  1  ms;
z =  1 min;
u =  1  hr;

print  "x = ", x, "\n";
print  "y = ", y, "\n";
print  "z = ", z, "\n";
print  "u = ", u, "\n";

w = x + y + z + u;
print "w = ", w, "\n";

/* =====================*/
/* UNITS of TEMPERATURE */
/* =====================*/
/* case 1: SI system   */

print "\n TEMPERATURE UNITS : SI \n";
x = 1 deg_C;
print  "x = ", x, "\n";

/* case 2: US system   */

print "\n TEMPERATURE UNITS : US \n";

x = 1 deg_F;
print  "x = ", x, "\n";

/* ===========================*/
/* UNITS of FREQUENCY & SPEED */
/* ===========================*/

x = 1 Hz;
y = 1 rpm;     /* rev. per min  */
z = 1 cps;     /* cycle per sec */

print "\n FREQUENCY & SPEED UNITS \n";

print  "x = ", x, "\n";
print  "y = ", y, "\n";
print  "z = ", z, "\n";

/* ===========================*/
/* UNITS of FORCE             */
/* ===========================*/

print "\n FORCE UNITS : SI \n";
x =  1   N;
y =  1  kN;
z =  1 kgf;
print  "x = ", x, "\n";
print  "y = ", y, "\n";
print  "z = ", z, "\n";

W = z + y + x;
print "W = ", W, "\n";

print "\n FORCE UNITS : US \n";
x =  1  lbf;
y =  1 kips;
print  "x = ", x, "\n";
print  "y = ", y, "\n";

W = x + y;
print "W = ", W, "\n";

/* ===========================*/
/* UNITS of PRESSURE          */
/* ===========================*/

print "\n PRESSURE UNITS : SI \n";
x =  1   Pa;
y =  1  kPa;
z =  1  MPa;
u =  1  GPa;
print  "x = ", x, "\n";
print  "y = ", y, "\n";
print  "z = ", z, "\n";
print  "u = ", u, "\n";

w = u + z + y + x ;
print "w = ", w, "\n";

print "\n PRESSURE UNITS : US \n";
x =  1  psi;
y =  1  ksi;

print  "x = ", x, "\n";
print  "y = ", y, "\n";
w = y + x ;
print "w = ", w, "\n";

/* ===========================*/
/* UNITS of ENERGY            */
/* ===========================*/

print "\n ENERGY UNITS : SI \n";
x =  1 Jou;
y =  1  kJ;
print  "x = ", x, "\n";
print  "y = ", y, "\n";

w = y + x ;
print "w = ", w, "\n";

/* ===========================*/
/* UNITS of POWER             */
/* ===========================*/
print "\n POWER UNITS : SI \n";
x =  1 Watt;
y =  1  kW;
print  "x = ", x, "\n";
print  "y = ", y, "\n";

w = y + x ;
print "w = ", w, "\n";

/* ===========================*/
/* UNITS of PLANE ANGLE       */
/* ===========================*/
print "\n PLANE ANGLE UNITS \n";
x =  1 deg;
y =  1 rad;
print  "x = ", x, "\n";
print  "y = ", y, "\n";

w = y + x ;
print "w = ", w, "\n";

/* ===============================*/
/* MAKE A QUANTITY DIMENSIONLESS  */
/* ===============================*/

print "\n QUANTITY DIMENSIONLESS \n";
x = 1 N;
y = 1 kN;
z = QDimenLess(x);
u = QDimenLess(y);

print  "                 x = ", x, "\n";
print  "                 y = ", y, "\n";
print  "DimensionLess of x = ", z, "\n";
print  "DimensionLess of y = ", u, "\n";
