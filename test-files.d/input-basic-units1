/* =====================================================================  */
/* This input file tests the variables with different Units               */
/* =====================================================================  */

/* ====================*/
/* UNITS of LENGTH     */
/* ====================*/

/* case 1: SI system   */

print " LENGTH UNITS : SI \n";
x =  1 mm;
y =  1 cm;
z =  1 dm;
u =  1  m;
v =  1 km;

print  "x = ", x, "\n";
print  "y = ", y, "\n";
print  "z = ", z, "\n";
print  "u = ", u, "\n";
print  "v = ", v, "\n";

w = x + y + z + u + v;
print "w = ", w, "\n";
w = y + z + u + v + x;
print "w = ", w, "\n";

/* case 2: US system   */

print "\n LENGTH UNITS : US \n";
x =  1       in;
y =  1      mil;
z =  1 micro_in;
u =  1       ft;
v =  1     yard;
w =  1     mile;


print  "x = ", x, "\n";
print  "y = ", y, "\n";
print  "z = ", z, "\n";
print  "u = ", u, "\n";
print  "v = ", v, "\n";
print  "w = ", w, "\n";

W = x + y + z + u + v + w;
print "W = ", W, "\n";

W = y + z + u + v + w + x;
print "W = ", W, "\n";

/* ====================*/
/* UNITS of VOLUME     */
/* ====================*/
print "\n VOLUME UNITS : US \n";
x =  1       gallon;
y =  1       barrel;
print  "x = ", x, "\n";
print  "y = ", y, "\n";

/* ====================*/
/* UNITS of MASS       */
/* ====================*/
/* case 1: SI system   */

print "\n MASS UNITS : SI \n";
x = 1 g;
y = 1 kg;
z = 1 Mg;
print  "x = ", x, "\n";
print  "y = ", y, "\n";
print  "z = ", z, "\n";

W = x + y + z;
print "W = ", W, "\n";

/* case 2: US system   */

print "\n MASS UNITS : US \n";

x = 1 lb;
y = 1 grain;
z = 1 klb;
u = 1 ton;
print  "x = ", x, "\n";
print  "y = ", y, "\n";
print  "z = ", z, "\n";
print  "u = ", u, "\n";

W = x + y + z + u;
print "W = ", W, "\n";



