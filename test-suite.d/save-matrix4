print "\n";
print "*** Extract a matrix element from a matrix \n";
print "========================================== \n";

X = [ 3, 4, 5; 6, 79, 100; 3, 4, 5];
X = RowUnits(X, [km], [1]);
X = RowUnits(X, [sec], [3]);
X = ColumnUnits(X, [MPa], [1]);
X = ColumnUnits(X, [N*m], [3]);

PrintMatrix(X);

print "\n   X[1][1] = ", X[1][1], "\n";
print "   X[1][2] = ", X[1][2], "\n";
print "   X[1][3] = ", X[1][3], "\n";
print "   X[2][1] = ", X[2][1], "\n";
print "   X[2][2] = ", X[2][2], "\n";
print "   X[2][3] = ", X[2][3], "\n";
print "   X[3][1] = ", X[3][1], "\n";
print "   X[3][2] = ", X[3][2], "\n";
print "   X[3][3] = ", X[3][3], "\n";

print "\n==== Assign matrix element with a quantity of same units \n";

X[1][1] = 2*X[1][1];
X[1][2] = 2*X[1][2];
X[1][3] = 2*X[1][3];
X[2][1] = 2*X[2][1];
X[2][2] = 2*X[2][2];
X[2][3] = 2*X[2][3];
X[3][1] = 2*X[3][1];
X[3][2] = 2*X[3][2];
X[3][3] = 2*X[3][3];

PrintMatrix(X);
print "\n==== Assign matrix element with a quantity of different units \n";
X[1][1] = 2 m;
print "\n   X[1][1] = ", X[1][1], "\n";
print "\n==== Assign a non-dimensional matrix element with a quantity with units \n";
X[2][2] = 2 m;
print "\n   X[2][2] = ", X[2][2], "\n";
PrintMatrix(X);

print "\n==== Cast a 1x1 matrix into a quantity \n";

X = [100 kPa];

PrintMatrix(X);
print "\n X = ", QuanCast(X), "\n";

